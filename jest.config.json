{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "moduleNameMapper": {
    "@exceptions/(.*)": "<rootDir>/src/exception/$1",
    "@entities/(.*)": "<rootDir>/src/v1/entities/$1",
