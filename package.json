{
  "license": "ISC",
  "version": "1.0.0",
  "name": "currency-converter-api",
  "description": "A currency converter API",
  "author": "Henrique Barros <henriquebarrosx@gmail.com>",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir build --copy-files --no-copy-ignored",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll",
    "start": "node build/index.js",
    "dev:watch": "nodemon -r tsconfig-paths/register ./src/index.ts",
    "dev": "ts-node -r tsconfig-paths/register ./src/index.ts",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/node": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.3",
    "@types/pino": "^7.0.5",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.26.0",
    "jest": "^29.2.1",
    "prisma": "^4.5.0",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@prisma/client": "^4.5.0",
    "axios": "^1.1.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "pino": "^8.7.0",
    "swagger-ui-express": "^4.5.0"
  }
}
